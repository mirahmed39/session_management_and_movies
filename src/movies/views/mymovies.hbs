<h1 class="display-6 home-header">Movies Added by the User</h1><br>
<table class="table table-striped table-bordered table-hover">
    <thead class="thead-inverse">
    <tr>
        <th>Title</th>
        <th>Year</th>
        <th>Director</th>
    </tr>
    </thead>
    <tbody>
    {{#each movies as |movie|}}
        <tr>
            <td>{{movie.title}}</td>
            <td>{{movie.year}}</td>
            <td>{{movie.director}}</td>
        </tr>
    {{/each}}
    </tbody>
</table>